{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "target": "esnext",
    "module": "esnext",
    "typeRoots": ["dist"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "types": ["vite/client"],
    "paths": {
      "*": ["../dist"]
    }
  }
}
